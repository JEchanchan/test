<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!-- 学校 -->
<hibernate-mapping>
    <class name="com.yunlu.model.SchHomepageModule" table="y_sch_homepage_module" >
        <id name="id" type="integer">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="schId" type="integer">
            <column name="sch_id"  not-null="true">
                <comment>学校ID</comment>
            </column>
        </property>
        <property name="moduleName" type="string">
            <column name="module_name">
                <comment>模块名称</comment>
            </column>
        </property>
        <property name="moduleType" type="integer">
            <column name="module_type">
                <comment>模块类型</comment>
            </column>
        </property>
        <property name="moduleVisable" type="integer">
            <column name="module_visable">
                <comment>模块可见性</comment>
            </column>
        </property>
        <property name="sortIndex" type="integer">
            <column name="sort_index">
                <comment>排序</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
